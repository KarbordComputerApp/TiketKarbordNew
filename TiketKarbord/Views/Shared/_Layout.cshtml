<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="PRAGMA" content="NO-CACHE">
    <meta content="width=device-width, initial-scale=1" name="viewport" />
    <title id="titlePage">@ViewBag.Title</title>
    <link rel="icon" href="~/Content/new/img/loading.png" type="image/x-icon" />
    <link href="~/Content/new/css/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/new/css/materialize.min.css" rel="stylesheet" />
    <link href="~/Content/new/js/jquery ui/jquery-ui.min.css" rel="stylesheet" />
    <link href="~/Content/new/css/style.css" rel="stylesheet" />
    <link href="~/Content/new/css/all-themes.css" rel="stylesheet" />
    <link href="~/Content/new/js/jquery.md.bootstrap.datetimepicker.style.css" rel="stylesheet" />
    <link href="~/Content/new/css/site.css?dt=@(TiketKarbord.Controllers.Unit.UnitPublic.titleVerNumber)" rel="stylesheet" />
    <link href="~/Content/new/css/dx.light.css" rel="stylesheet" />
    <link href="~/Content/new/css/mapp.min.css" rel="stylesheet" />
</head>
<body class="light rtlSite" ondragstart="return false;" ondrop="return false;">

    <div id="loadingsite" class="page-loader-wrapper">
        <div class="loader">
            <div class="m-t-30">
                <img class="loading-img-spin" src="~/Content/loading1.png" width="20" height="20" alt="admin">
            </div>
            <p data-bind="text:'لطفا منتظر بمانید'"></p>
        </div>
    </div>




    <div class="overlay"></div>
    <nav class="navbar">
        <div class="container-fluid">
            <div class="navbar-header">
                <a href="javascript:void(0);" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar-collapse"
                   aria-expanded="false"></a>
                <a href="javascript:void(0);" class="bars"></a>
                <a class="navbar-brand">

                    <!--<img src="~/Content/img/LLL.png" width="30" />
                    <span class="logo-name">کاربرد کامپیوتر</span>-->
                </a>
            </div>
            <div class="collapse navbar-collapse" id="navbar-collapse">



                <ul class="pull-right" id="navbar_Buttom" style="padding-right: 0px;">
                    <li>
                        <a href="javascript:void(0);" class="sidemenu-collapse">
                            <i class="material-icons">reorder</i>
                        </a>
                    </li>
                </ul>

                <ul class="nav navbar-nav navbar-right" id="navbar_Menu">

                    <li class="dropdown user_profile pull-left">
                        <a href="javascript:void(0);" class="dropdown-toggle" data-toggle="dropdown" role="button" style="margin-left: 50px;">
                            <img src="~/Content/img/list/profile.png" width="32" height="32" alt="User" style="border: #ec8121 2px solid;" />
                            <span id="userNameFa"></span>
                            <img src="~/Content/img/streamline-icon-arrow-button-down-2@48x48.png" width="10" height="10" />
                        </a>
                        <ul class="dropdown-menu pullDown" style="margin-left: 10px; width:250px">
                            <li class="body">
                                <ul class="user_dw_menu" style="padding-right:0px">
                                    <li style="width: 100%;">
                                        <div class="row" style="padding:15px; padding-top:25px">
                                           <!-- <div class="col-sm-12">
                                                <div class="form-group form-float">
                                                    <div class="form-line date fix">
                                                        <input type="text" id="coName_TitleMenu" class="form-control float" readonly>
                                                        <label class="form-label" data-bind="text:'شرکت'"></label>
                                                    </div>
                                                </div>
                                            </div>

                                            <div class="col-sm-12">
                                                <div class="form-group form-float">
                                                    <div class="form-line date fix">
                                                        <input type="text" id="sal_TitleMenu" class="form-control float" readonly>
                                                        <label class="form-label" data-bind="text:'سال'"></label>
                                                    </div>
                                                </div>
                                            </div>-->

                                            <div class="col-sm-12">
                                                <button data-bind="text:'خروج'" type="button" id="LogOut" class="btn btn-primary btn-block" style="background-color: #eb8121 !important;"></button>
                                            </div>

                                        </div>
                                    </li>
                                </ul>

                            </li>
                        </ul>

                    </li>

                    <li style="color: #e77e22;font-size: 35px;margin-top: 5px;margin-right: 8px;" class="pull-left">|</li>


                    <li class="pull-left" id="P_Home">
                        <a href="/Home/Index" class="dropdown-toggle" data-close="true" title="خانه">
                            <img src="~/Content/img/streamline-icon-house-2@48x48.png" width="16" height="16" />
                        </a>
                    </li>



                    <li class="dropdown pull-left notification" id="P_NotificationErja">
                        <a href="/ERJ/Erja" title="تیکت دریافتی">
                            <img id="" src="~/Content/img/notificationErja.png" width="23" height="25" style="margin-top: 1px;" />
                            <span class="badge" id="notificationCount"></span>
                        </a>
                    </li>


                    <li class="dropdown pull-left" id="P_ContactUs" title="تماس با ما">
                        <a id="btn_App" href="https://karbordcomputer.com/contact-us/">
                            <img src="~/Content/img/ContactUs.png" width="18" height="18" style="margin-top: 5px;margin-bottom: 0px;" />
                        </a>
                    </li>


                    <li class="dropdown pull-left" id="P_App" title="راهنمای کلید های میانبر">
                        <a id="HotKeyHelp" class="dropdown-toggle" data-toggle="modal" data-backdrop="static" data-keyboard="false" data-target="#modal-HotKeyHelp">
                            <img src="~/Content/img/keyboard.png" width="32" style="margin-top: 10px;" />
                        </a>
                    </li>

                </ul>
            </div>
        </div>
    </nav>



    <aside id="leftsidebar" class="sidebar">
        <div class="menu">
            <ul class="list">
                <li class="sidebar-user-panel active">
                    <div class="center">
                        <img id="logoMenu" src="~/Content/img/logo meno2.png" style="width: 100px;" />

                        <img src="~/Content/img/new item/line.png" style="padding :5px 0px; max-width:100%" />
                    </div>
                    <div class="profile-usertitle">
                        <!--<div class="sidebar-userpic-name" id="TextUserName"></div>
            <div class="profile-usertitle-job " id="TextNoUser"></div>-->
                        <div class="profile-usertitle-job ">
                            <span id="titleVer">@(TiketKarbord.Controllers.Unit.UnitPublic.titleVer) :</span>
                            <span id="titleVerNumber" style="padding-right:5px">@(TiketKarbord.Controllers.Unit.UnitPublic.titleVerNumber)</span>
                            <br />
                            <span id="titleVer">ورژن سرویس : 521</span>
                            <span id="titleVerNumber" style="padding-right:5px"></span>
                            <br />
                        </div>

                    </div>
                </li>


                <li id="Erj_Menu" class="active">
                    <a href="javascript:void(0);" class="menu-toggle">
                        <i>
                            <img src="~/Content/img/SanadAnbarWhite.png" width="20" />
                        </i>
                        <span>ارجاعات</span>
                    </a>
                    <ul class="ml-menu">
                        <li id="ErjDOC" class="rightClick">
                            <a href=@Url.Action("Index","Erj")>پرونده</a>
                        </li>
                        <li id="Erj_Resive" class="rightClick">
                            <a href=@Url.Action("Erja","Erj")>ارجاعات پرونده دریافتی</a>
                        </li>
                        <li id="Erj_Send" class="rightClick">
                            <a href=@Url.Action("Erja","Erj")>ارجاعات پرونده ارسالی</a>
                        </li>

                        <li id="Dayjob" class="rightClick">
                            <a href=@Url.Action("Dayjob","Erj")>گزارش کار</a>
                        </li>
                    </ul>
                </li>


                <li id="Tiket_Menu">
                    <a href="javascript:void(0);" class="menu-toggle">
                        <i>
                            <img src="~/Content/img/SanadAnbarWhite.png" width="20" />
                        </i>
                        <span>تیکت</span>
                    </a>
                    <ul class="ml-menu">
                        <li id="TiketDOC" class="rightClick">
                            <a href=@Url.Action("Index","Tiket")>تیکت ها</a>
                        </li>
                        <li id="Tiket_Resive" class="rightClick">
                            <a href=@Url.Action("Tiket","Tiket")> تیکت دریافتی</a>
                        </li>
                        <li id="Tiket_Send" class="rightClick">
                            <a href=@Url.Action("Tiket","Tiket")> تیکت ارسالی</a>
                        </li>
                    </ul>
                </li>

            </ul>
        </div>
    </aside>


    <section id="content" class="content">
        <div class="container-fluid">
            @RenderBody()
        </div>

    </section>

    <div class="modal fade" id="modal-Message" tabindex="-1" role="dialog" aria-labelledby="formModal" aria-hidden="true" data-backdrop="static" data-keyboard="false">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button data-bind="attr: {title:text_Close}" type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">×</span>
                    </button>
                    <h4 class="modal-title" id="titleMessage"></h4>
                    <a></a>
                </div>
                <div class="modal-body">
                    <textarea id="bodyMessage" rows="30" class="form-control no-resize auto-growth string" style="text-align:justify"></textarea>
                </div>
            </div>

        </div>
    </div>


    <div class="modal fade" id="modal-Comm" tabindex="-1" role="dialog" aria-labelledby="formModal" aria-hidden="true" data-backdrop="static" data-keyboard="false">
        <div class="modal-dialog" id="modal-dialogComm" style="top: 150px;">
            <div class="modal-content">
                <div class="modal-header" style="padding-bottom: 0px;">
                    <div>
                        <button data-bind="attr: {title:text_Close}" type="button" class="close" data-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true">&times;</span>
                        </button>
                        <a id="minMaxComm">
                            <img id="imgComm" src="/Content/img/new item/square-svgrepo-com.svg" style="width:16px">
                        </a>
                    </div>
                    <h4 class="modal-title" id="titleComm">توضیحات</h4>
                    <div>
                      
                    </div>

                </div>
                <div class="modal-body">
                    <span id="codeComm" style="display: none"></span>
                    <textarea id="commPublic" rows="20" class="form-control no-resize auto-growth string autocomplete" style="text-align:justify" readonly>
                </textarea>
                </div>
            </div>
        </div>
    </div>




    <div class="modal fade" id="modal-HotKeyHelp" tabindex="-1" role="dialog" aria-labelledby="formModal" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button data-bind="attr: {title:text_Close}" type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                    <h4 class="modal-title" data-bind="text:'راهنمای کلید های میانبر'"></h4>
                    <a></a>
                </div>
                <div class="modal-body">
                    <div class="row">
                        <table style="margin-top: 10px;" class="table-hover">
                            <thead>
                                <tr style="background-color:#d9d9d9">
                                    <th style="padding: 5px;">کلید میانبر</th>
                                    <th style="padding: 5px;">شرح</th>
                                </tr>

                            </thead>
                            <tbody>
                                <tr style="border: #dddddd 1px solid;">
                                    <td style="padding: 5px;">Ctrl + Insert</td>
                                    <td style="padding: 5px;">جدید </td>
                                </tr>

                                <tr style="border: #dddddd 1px solid;">
                                    <td style="padding: 5px;">Esc</td>
                                    <td style="padding: 5px;">انصراف</td>
                                </tr>
                                <tr style="border: #dddddd 1px solid;">
                                    <td style="padding: 5px;">F2</td>
                                    <td style="padding: 5px;">ذخیره</td>
                                </tr>
                                <tr style="border: #dddddd 1px solid;">
                                    <td style="padding: 5px;">Ctrl + F3</td>
                                    <td style="padding: 5px;">جستجو</td>
                                </tr>

                                <tr style="border: #dddddd 1px solid;">
                                    <td style="padding: 5px;">Alt + P</td>
                                    <td style="padding: 5px;">چاپ</td>
                                </tr>

                                <tr style="border: #dddddd 1px solid;">
                                    <td style="padding: 5px;">Alt + R</td>
                                    <td style="padding: 5px;">بند جدید</td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>



    <script type="text/javascript">
        sessionStorage.versionTitle = '@(TiketKarbord.Controllers.Unit.UnitPublic.titleVer)';
    </script>


    <script src="~/Content/new/js/app.min.js"></script>
    <script src="~/Content/new/js/admin.js"></script>
    <script src="~/Content/js/knockout-3.5.0.js"></script>
    <script src="~/Content/js/jalaali.js"></script>
    <script src="~/Scripts/app.js?dt=@(TiketKarbord.Controllers.Unit.UnitPublic.titleVerNumber)"></script>
    <script src="~/Scripts/PublicAFI.js?dt=@(TiketKarbord.Controllers.Unit.UnitPublic.titleVerNumber)"></script>
    <script src="~/Content/js/sweet2.js"></script>

    <script src="~/Content/new/js/jquery.md.bootstrap.datetimepicker.js"></script>
    <script src="~/Content/new/js/jquery.md5.js"></script>
    <script src="~/Content/new/js/farsiInput.js"></script>

    @RenderSection("scripts", false)

</body>
</html>
